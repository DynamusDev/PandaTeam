{
  "expo": {
    "name": "Panda Cash&Fun",
    "slug": "mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification":{
        "icon":"./assets/icon.png"
      },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#55D500"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.dynamusdev.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.dynamusdev.mobile",
      "useNextNotificationsApi": true,
      "icon": "./assets/icon.png"
    }
  }
}
